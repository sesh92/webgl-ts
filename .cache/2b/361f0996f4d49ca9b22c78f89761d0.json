ers.forEach((listener: IJoystickListener) => {\n      if (code === 'KeyA') {\n        listener.offLeft();\n      }\n      if (code === 'KeyD') {\n        listener.offRight();\n      }\n    });\n  };\n\n  init = () => {\n    document.addEventListener('keydown', this.keyDown);\n    document.addEventListener('keyup', this.keyUp);\n  };\n\n  subscribe = (listener: IJoystickListener) => this.listeners.push(listener);\n\n  clear = () => {\n    document.removeEventListener('keydown', this.keyDown);\n    document.removeEventListener('keyup', this.keyUp);\n    this.listeners = [];\n  };\n}\n\nconst Instance = new JoystickEvent();\n\nexport default Instance;\n"},"lineCount":null}},"error":null,"hash":"03e2345b46c58f6dc59858b137c97548","cacheData":{"env":{}}}